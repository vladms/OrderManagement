package ro.utcn.pt.bonta.presentation;

import ro.utcn.pt.bonta.model.Customer;

public interface AppInterfaceButtonEvents {
	 public void newUserCreated (String cnp, String name, String age);
	 public void userEdited (Customer selectedCustomer, String cnp, String name, String age);

	 public void newProductCreated (String id, String name, double price);
	 public void productEdited (Product selectedProduct, String id, String name, double price);


}
